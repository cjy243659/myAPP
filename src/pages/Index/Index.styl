@import("../../stylus/index.styl");

.fater
    padding-bottom: $HeaderHeight
.navFooter
    width 100vw
    height $cateHeight
    display: flex
    background: $color-fff
    position fixed
    left 0
    bottom 0 
    border-top $borderWidth solid $color-ccc
    a
        flex: 1 
        text-align: center 
        font-size $h7
        line-height: $margin1
        img 
            width $footerimgW
            height $footerimgW
            display: block 
            margin: $margin auto 
    .navFooter-style
        color $primary